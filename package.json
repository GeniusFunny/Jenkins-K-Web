{
  "private": true,
  "scripts": {
    "dev": "umi dev",
    "build": "umi build",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@umijs/test": "^3.5.36",
    "lint-staged": "^10.0.7",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "terminal-in-react": "^4.3.1",
    "umi": "^3.5.36",
    "whatwg-fetch": "^3.0.0",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@umijs/preset-react": "^1.4.8"
  }
}
